$(document).ready(function() {
      var boxTarget = $('.box-target');
  
      // Reusable function for loading content
      function loadContent(url) {
          $.ajax({
              type: "GET",
              url: url, // URL for airquality.html
              success: function(data) {
                  var highchartCode = $(data).find('#container').html();
                  boxTarget.html(highchartCode);
              },
              error: function(jqXHR, textStatus, errorThrown) {
              console.error("Error: " + errorThrown);
              }
          });
      }
  
      // Attach click event handler to the "Air Quality" button
      $('#id_btn_airquality').click(function() {
          // Specify the URL of the airquality.html file
          var airqualityUrl = "{{ url_for('vash.page_air_quality') }}"; // Replace with the actual URL
  
          // Call the loadContent function with the airqualityUrl
          loadContent(airqualityUrl);
      });
  
      // Add similar click event handlers for other buttons as needed
    });
